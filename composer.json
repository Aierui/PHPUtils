{
  "name": "aierui/phputils",
  "description": "This component provides useful PHP functions, mini classes and snippets that you need and can use every day.",
  "keywords": [
    "php",
    "utils",
    "phputils"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "aierui",
      "email": "aieruishi@gmail.com",
      "homepage": "https://shijinrong.cn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5"
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  },
  "autoload": {
    "psr-4": {
      "PHPUtils\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PHPUtils\\Tests\\": "tests/"
    }
  },
  "homepage": "https://github.com/Aierui/PHPUtils",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Aierui/PHPUtils"
    }
  ],
  "support": {
    "issues": "https://github.com/Aierui/PHPUtils/issues",
    "wiki": "https://github.com/Aierui/PHPUtils/#documentation"
  }
}
